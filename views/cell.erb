<% colour_class = colour_class(@check_solution, @puzzle[cell_index], @current_solution[cell_index], @solution[cell_index]) %>
<section class="cell">
  <% cell_value = cell_value(@current_solution[cell_index]) %>
  <% if  colour_class == "value-provided" %>
    <input name="cell[]" type="text" maxlength="1" autocomplete="off" value="<%= cell_value %>" class="<%= colour_class %>" readonly>
  <% else %>
    <input name="cell[]" type="text" maxlength="1" autocomplete="off" value="<%= cell_value %>" class="<%= colour_class %>">
  <% end %>
</section>