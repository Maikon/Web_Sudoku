<% 3.times do |row| %>
  <section class="row">
    <% 3.times do |box| %>
      <section class="box">
        <% 3.times do |cell_row| %>
          <section class="cell_row">
            <% 3.times do |cell| %>
              <%= partial :cell, :locals=>{:cell_index=>row * 27 + box * 3 + cell_row * 9 + cell}  %>
            <% end %>
          </section>
        <% end %>
      </section>
    <% end %>
  </section>
<% end %>
